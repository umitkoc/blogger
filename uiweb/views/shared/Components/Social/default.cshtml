@model ICollection<Social>

@if(Model.Count()!=0){
    @foreach (var item in Model)
{
    <a href=@item.SocialUrl target="_blank" style="text-decoration: none;color:black">
        <i class=@item.IconName></i>
    </a>
}

<a data-bs-toggle="modal" data-bs-target="#exampleModal"> <i class="fas fa-file-pdf fa-2x"></i></a>
}else{
      <div class="alert alert-danger">Not Social</div>
}